<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.BlogGUIController">
<children>
   <Button fx:id="registerButton" layoutX="266.0" layoutY="260.0" mnemonicParsing="false" onAction="#register" text="Register" />
   <Button fx:id="loginButton" layoutX="266.0" layoutY="217.0" mnemonicParsing="false" onAction="#login" prefHeight="27.0" prefWidth="69.0" text="Login" />
   <Label layoutX="155.0" layoutY="48.0" text="Welcome to BlogPost" textFill="#837d7d">
      <font>
         <Font name="Bodoni 72 Bold" size="34.0" />
      </font>
   </Label>
   <TextField fx:id="usernameField" layoutX="217.0" layoutY="129.0" promptText="Username" />
   <TextField fx:id="passwordField" layoutX="217.0" layoutY="165.0" promptText="Password" />
</children>
</AnchorPane>
